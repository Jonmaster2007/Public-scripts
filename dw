);local v366=v0:IsKeyDown(Enum.KeyCode.LeftShift);if ((v363.KeyCode==Enum.KeyCode.RightBracket) and v365 and v366) then local v480=v68:FindFirstChild("activeHostiles") and v68.activeHostiles:FindFirstChild("AI_QUEEN") ;local v481=v480 and v480:FindFirstChild("Head") ;local v482=v480 and v480:FindFirstChildOfClass("Humanoid") ;if (v480 and v481 and v482) then v481.CanCollide=false;v481.Transparency=1;v481.Massless=true;if v175 then v175:Disconnect();v175=nil;end if  not v164 then v165=v481.Size;v481.Size=Vector3.new(45,200,45);v164=true;v173(v480);print("🟣 SQhead Sized 200 and outlined");v172(v480,v482.Health,v482.MaxHealth,(v167 and v167.OutlineColor) or Color3.fromRGB(158,60,214) );if  not v78:IsBoundToRenderStep("UpdateQueenHealth") then v78:BindToRenderStep("UpdateQueenHealth",Enum.RenderPriority.Last.Value,function() if (v482.Health>0) then v172(v480,v482.Health,v482.MaxHealth,(v167 and v167.OutlineColor) or Color3.fromRGB(158,60,214) );else v78:UnbindFromRenderStep("UpdateQueenHealth");v171();end end);end if proximityCheckTask then proximityCheckTask:Disconnect();end proximityCheckTask=task.spawn(function() while v164 and v481 and v481.Parent  do local v552=v98 and v98.Character ;local v553=v552 and v552:FindFirstChild("HumanoidRootPart") ;if v553 then local v565=(v481.Position-v553.Position).Magnitude;if (v565<=55) then if v166 then v166:Disconnect();v166=nil;end v174(v481);v172(v480,v482.Health,v482.MaxHealth,Color3.fromRGB(158,60,214));break;end end task.wait(0.5);end end);if v166 then v166:Disconnect();end v166=task.delay(150,function() if (v164 and v481 and v481.Parent) then v174(v481);end if (v482 and (v482.Health<=0)) then v78:UnbindFromRenderStep("UpdateQueenHealth");v171();end if proximityCheckTask then proximityCheckTask:Disconnect();proximityCheckTask=nil;end v166=nil;end);else if v166 then v166:Disconnect();v166=nil;end v174(v481);v78:UnbindFromRenderStep("UpdateQueenHealth");v171();end else v163:SetCore("SendNotification",{Title="AI_QUEEN Head Not 